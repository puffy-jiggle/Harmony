  "/*": "Development runs on webpack-dev-server port 3033, with API server on 8080",
  "/**": "Use 'npm run dev' for development, which starts both servers and Tailwind",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm start\" \"npm run start:server\" \"npm run tailwind\"",
    "start": "webpack serve --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "start:server": "nodemon src/server/server.ts",
    "type-check": "tsc --noEmit",
    "tailwind": "tailwindcss -i ./src/input.css -o ./public/output.css --watch",
    "test:client": "jest -c jest.client.config.ts",
    "test:server": "jest -c jest.server.config.ts",
    "test": "npm run test:client && npm run test:server"
  },
